kind: GitRepo
apiVersion: fleet.cattle.io/v1alpha1
metadata:
  name: simple
  namespace: fleet-local
spec:
  repo: https://git.tdology.com/xiaohang/gittest/
  clientSecretName: hang-ssh-auth
  paths:
  - simple



apiVersion: v1
kind: Secret
metadata:
  name: hang-ssh-auth
  namespace: fleet-local
type: kubernetes.io/ssh-auth
data:
  # the data is abbreviated in this example
  ssh-privatekey: |
     



apiVersion: apps/v1
kind: Deployment
metadata:
  name: busybox
spec:
  selector:
    matchLabels:
      app: busybox
  template:
    metadata:
      labels:
        app: busybox
    spec:
      volumes:
      - name: volume-test
        emptyDir: {}
      containers:
      - name: app
        image: hub-mirror.c.163.com/library/busybox
        volumeMounts:
        - name: volume-test
          mountPath: /datatest
          readOnly: true
        resources:
          limits:
            cpu: 100m
            memory: 64M
          requests:
            cpu: 100m
            memory: 64M
        args:
        - top